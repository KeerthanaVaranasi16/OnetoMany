{
  "name": "onetomany",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start:dev": "nodemon ./src/app.ts",
    "build": "tsc -p .",
    "start:prod": "node ./dist/app.js",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run -d ./src/DataSource/dataSource.ts",
    "migration:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert -d ./src/DataSource/dataSource.ts",
    "schema:sync": "npm run typeorm schema:sync -- -d src/DataSource/dataSource.ts",
    "migration:show": "npm run typeorm migration:show -- -d src/DataSource/dataSource.ts",
    "migration:generate": "npm run typeorm migration:generate -- -d src/DataSource/dataSource.ts",
    "migration:create": "npm run typeorm migration:create"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.1",
    "typeorm": "^0.3.19"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.5",
    "nodemon": "^3.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "description": ""
}
